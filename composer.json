{
    "name": "php-integrator/core",
    "description": "PHP code analysis and utilities",
    "keywords": ["static", "analysis", "indexing", "indexer", "integrator", "autocompletion", "refactoring", "tooltips", "signature-help"],
    "homepage": "https://php-integrator.github.io",
    "license": "GPL-3.0",
    "require": {
        "php": ">=7.1.0",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "php-integrator/docblock-type-parser": "dev-master#a85867b843a4b6811b95b908d4be5614e27f84e8",
        "doctrine/cache": "1.6.*",
        "doctrine/dbal": "2.5.*",
        "evenement/evenement": "2.0.*",
        "nikic/php-parser": ">=3.0.5 <3.1.0",
        "react/socket": "0.7.*",
        "symfony/yaml": "3.2.*",
        "symfony/config": "3.2.*",
        "symfony/dependency-injection": "3.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.1.*",
        "symfony/var-dumper": "3.2.*",
        "martinsik/php-doc-parser": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "PhpIntegrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpIntegrator\\Tests\\": "tests/"
        }
    }
}
