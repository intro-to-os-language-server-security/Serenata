{
    "name": "php-integrator/core",
    "description": "PHP code analysis and utilities",
    "keywords": ["static", "analysis", "indexing", "indexer", "integrator", "autocompletion", "refactoring"],
    "homepage": "https://php-integrator.github.io",
    "license": "GPL-3.0",
    "require": {
        "php": ">=7.1.0",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "doctrine/cache": "1.6.*",
        "doctrine/dbal": "2.5.*",
        "evenement/evenement": "2.0.*",
        "nikic/php-parser": ">=3.0.5 <3.1.0",
        "react/socket": "0.5.*",
        "symfony/yaml": "3.2.*",
        "symfony/config": "3.2.*",
        "symfony/dependency-injection": "3.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*"
    },
    "autoload": {
        "psr-4": {
            "PhpIntegrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpIntegrator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test" : "vendor/bin/phpunit"
    }
}
