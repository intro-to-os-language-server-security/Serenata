{
    "name": "php-integrator/core",
    "description": "PHP code analysis and utilities",
    "keywords": ["static", "analysis", "indexing", "indexer", "integrator", "autocompletion", "refactoring", "tooltips", "signature-help"],
    "homepage": "https://php-integrator.github.io",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "doctrine/cache": "^1.7",
        "doctrine/orm": "^2.6",
        "evenement/evenement": "^3.0",
        "jetbrains/phpstorm-stubs": "dev-master",
        "league/html-to-markdown": "^4.6",
        "nikic/php-parser": "v4.0.0",
        "php-ds/php-ds": "^1.2",
        "php-integrator/common": "^0.1",
        "php-integrator/docblock-type-parser": "^0.2",
        "php-integrator/name-qualification-utilities": "^0.3",
        "ramsey/uuid": "^3.7",
        "react/socket": "^0.8",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "brianium/paratest": "^1.1",
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PhpIntegrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpIntegrator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
