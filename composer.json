{
    "name": "serenata/serenata",
    "description": "Free and open source server providing code assistance for PHP",
    "keywords": ["static", "analysis", "indexing", "indexer", "ide", "autocompletion", "refactoring", "tooltips", "signature-help"],
    "homepage": "https://serenata.github.io",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "doctrine/cache": "^1.7",
        "doctrine/orm": "^2.6",
        "evenement/evenement": "^3.0",
        "jetbrains/phpstorm-stubs": "dev-master",
        "league/html-to-markdown": "^4.6",
        "nikic/php-parser": "dev-master#7b201b63d2cef4ec795d52044a31d20e79e871fb as v4.0.1",
        "php-ds/php-ds": "^1.2",
        "serenata/common": "^0.2",
        "serenata/docblock-type-parser": "^0.3.1",
        "serenata/name-qualification-utilities": "^0.4.1",
        "ramsey/uuid": "^3.7",
        "react/socket": "^0.8",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "brianium/paratest": "^1.1",
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Serenata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serenata\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
