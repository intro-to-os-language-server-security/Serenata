{
    "name": "serenata/serenata",
    "description": "Free and open source server providing code assistance for PHP",
    "keywords": ["static", "analysis", "indexing", "indexer", "ide", "autocompletion", "refactoring", "tooltips", "signature-help"],
    "homepage": "https://serenata.github.io",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "composer/xdebug-handler": "^1.3",
        "doctrine/cache": "^1.8",
        "doctrine/orm": "^2.6",
        "evenement/evenement": "^3.0",
        "jetbrains/phpstorm-stubs": "dev-master@dev",
        "league/html-to-markdown": "^4.8",
        "nikic/php-parser": "^4.2.2",
        "php-ds/php-ds": "^1.2",
        "react/socket": "^1.2",
        "serenata/common": "^0.3.3",
        "serenata/docblock-type-parser": "^0.4",
        "serenata/name-qualification-utilities": "^0.5",
        "symfony/config": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/finder": "^4.3",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "brianium/paratest": "^2.2",
        "phpstan/phpstan": "0.11.15",
        "phpstan/phpstan-doctrine": "0.11.5",
        "phpstan/phpstan-php-parser": "0.11.1",
        "phpstan/phpstan-phpunit": "0.11.2",
        "phpstan/phpstan-strict-rules": "0.11.1",
        "phpunit/phpunit": "^7.5",
        "slevomat/coding-standard": "5.0.4",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Serenata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serenata\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
