{
    "name": "php-integrator/core",
    "description": "PHP code analysis and utilities",
    "keywords": ["static", "analysis", "indexing", "indexer", "integrator", "autocompletion", "refactoring", "tooltips", "signature-help"],
    "homepage": "https://php-integrator.github.io",
    "license": "GPL-3.0",
    "require": {
        "php": ">=7.1.0",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "php-integrator/common": "dev-master#d554a26fe0ef3972f5f996a4a7838368b6cfb352",
        "php-integrator/name-qualification-utilities": "dev-master#b8eb7528cf0b5b4b14603f9ac16cc1fb58f557e5",
        "php-integrator/docblock-type-parser": "dev-master#a7e2ffdb99706b9bf5378ac184f5252fb5b98148",
        "doctrine/cache": "1.6.*",
        "doctrine/dbal": "2.5.*",
        "evenement/evenement": "2.0.*",
        "nikic/php-parser": "dev-master#92275bdfa8ffa8a3bd1df00eed9fb6627c880ba1",
        "react/socket": "0.7.*",
        "symfony/yaml": "3.2.*",
        "symfony/config": "3.2.*",
        "symfony/dependency-injection": "3.2.*",
        "ramsey/uuid": "3.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.1.*",
        "symfony/var-dumper": "3.2.*",
        "martinsik/php-doc-parser": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "PhpIntegrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpIntegrator\\Tests\\": "tests/"
        }
    }
}
